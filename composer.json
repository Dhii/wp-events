{
    "name": "dhii/wp-events",
    "description": "A PSR-14 compliant event wrapper for WordPress.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Dhii",
            "email": "development@dhii.co"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.3.9 | ^7.0",
        "dhii/psr-14": "~0.1"
    },
    "require-dev": {
        "dhii/php-cs-fixer-config": "dev-php-5.3",
        "phpunit/phpunit": "4.*",
        "10up/wp_mock": "dev-dev",
        "ptrofimov/xpmock": "^1.1",
        "mockery/mockery": "^0.9.5"
    },
    "autoload": {
        "psr-4": {
            "Dhii\\WpEvents\\" : "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Dhii/psr-14"
        }
    ]
}
